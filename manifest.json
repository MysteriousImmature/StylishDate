{
    "manifest_version": 3,
    "name": "StylishDate",
    "version": "1.0",
    "description": "Calendar",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "StylishDate@Auraaid.netlify.com",
            "strict_min_version": "42.0"
        }
    }
}
